spring:
  data:
    mongodb:
      user: DB_USERNAME
      password: DB_PASSWORD
      host: DB_HOST
      database: DB_DATABASE
  mvc:
    throw-exception-if-no-handler-found: false
  web:
    resources:
      add-mappings: false
  test:
    username: TEST_USERNAME
    password: TEST_PASSWORD
  jackson:
    deserialization:
      fail-on-unknown-properties: true
server:
  port: 8081
jwt:
  secret: LIVIZ_JWT_SECRET_KEY
